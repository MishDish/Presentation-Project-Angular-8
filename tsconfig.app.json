{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@@core/*": [
        "src/app/core/*"
      ],
      "@@core": [
        "src/app/core"
      ],
      "@@shared": [
        "src/app/shared"
      ],
      "@@shared/*": [
        "src/app/shared/*"
      ],
      "@@services/*": [
        "src/app/services/*"
      ]
    },
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}